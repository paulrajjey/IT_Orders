          <drools:script><![CDATA[Integer cnt = (Integer) kcontext.getVariable("count");
java.util.List aplist = (java.util.List ) kcontext.getVariable("ApprovalList");
if ( cnt == null){
     cnt = new Integer(0);
     kcontext.setVariable("count", 0 );
   
} else {
 kcontext.setVariable("count", cnt + 1 );
}
System.out.println("count" + cnt);
if( aplist != null && aplist.size() > 0 ) {
     System.out.println("size" + aplist.size());

  ApproverInfo info =   (ApproverInfo) aplist.get(cnt);
  kcontext.setVariable("approver",info.getName());
}]]></drools:script>
