package org.jbpm.demo.itorders;

import java.lang.Number;

rule "mgrApprovalLit"
	dialect "mvel"
	when
		ap : ApproverInfo( )
	then
		java.util.List aplist = kcontext.getVariable("");
		if ( aplist == null ) {
		aplist = new java.util.ArrayList();
		}
		aplist.add(ap);
		kcontext.setVariable("",aplist);
		kcontext.retract(app);
end
